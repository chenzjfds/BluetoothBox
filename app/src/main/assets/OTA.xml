<?xml version="1.0" encoding="utf-8"?>
<!--
    part        : 结点名, 定义一个分区描述项
    desc        : 分区描述
    ID          : 分区ID
    size        : 分区允许大小(十六进制表示，字节为单位)
    needbackup  : 是否需要先备份再写
    name        : 分区包含的文件集合
-->

<firmwareupdate name = "/">

  <!-- 第一个分区ID -->
  <part id = "1" desc = "brec是第一分区，允许最大大小64KB">
    <param size = '0x10000' needbackup = "1"/>   
  </part>  
  
  <!-- 第二个分区ID -->  
  <part id = "2" desc = "lfi是第二个分区，允许最大大小2M">
